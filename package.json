{
  "name": "React-Babel-Redux-Starter-Kit",
  "version": "0.0.1",
  "description": "starter kit using react, babel, redux, webpack, axios, and chai",
  "main": "''",
  "scripts": {
    "build": "NODE_ENV=\"'production'\" webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build_dev": "NODE_ENV=\"'develop'\" webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start": "NODE_ENV=\"'local'\" webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive $(find test -name '*.spec.js')"
  },
  "repository": {
    "type": "git"
  },
  "author": "Genki Sato",
  "license": "MIT",
  "dependencies": {
    "aurelia-i18n": "^1.2.1",
    "axios": "^0.15.2",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-runtime": "^6.11.6",
    "bootstrap": "^4.0.0-alpha.3",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "material-ui": "^0.16.1",
    "moment-timezone": "^0.5.11",
    "nkdash": "^1.1.3",
    "node-sass": "^3.13.1",
    "postcss-loader": "^1.0.0",
    "react": "15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-cookie": "^0.4.8",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "15.4.0",
    "react-dropzone": "^3.7.3",
    "react-highcharts": "^11.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-motion": "^0.4.5",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.2.5",
    "react-router": "^2.8.1",
