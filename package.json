  "devDependencies": {
    "babel-plugin-resolver": "^1.1.0",
    "chai": "^3.5.0",
    "css-loader": "0.25.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.2.0",
    "webpack": "1.13.2",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-server": "1.16.2"
  }
